{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "https://discord.com/app",
    "distDir": "https://discord.com/app",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Dexcord",
    "version": "0.0.4"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.dex.dev",
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "./src/injection/injection.js",
        "./html/index.html",
        "./html/notification.html",
        "./html/top.html",
        "./html/settings.html"
      ],
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    }
  }
}
